/*
-- Query: SELECT st.MonitoringLocationName, ac.ActivityStartDate, br.CharacteristicName, br.ResultMeasureValue, br.`ResultMeasure/MeasureUnitCode`
FROM  `12_741_Project`.`activityall` as ac INNER JOIN `12_741_Project`.`biologicalresult` as br 
ON br.ActivityIdentifier = ac.ActivityIdentifier
LEFT JOIN `12_741_Project`.`station12` as st    
ON ac.MonitoringLocationIdentifier = st.MonitoringLocationIdentifier
WHERE br.CharacteristicName = 'pH' and (br.ResultMeasureValue > 8.5 or br.ResultMeasureValue < 6)
LIMIT 0, 1000

-- Date: 2019-12-07 22:57
*/
